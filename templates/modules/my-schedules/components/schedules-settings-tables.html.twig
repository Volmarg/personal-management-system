<table id="MySchedulesTypes" class="table table-striped table-bordered" cellspacing="0" data-table="true" width="100%">
    <thead>
    <tr>
        <td class="d-none">{{ 'datatables.table.idHash' | trans }}</td>
        <td>{{ 'schedules.settings.table.name' | trans }}</td>
        <td>{{ 'schedules.settings.table.icon' | trans }}</td>
        <td>{{ 'datatables.table.actions' | trans }}</td>
    </tr>
    </thead>
    <tbody>
    {% for type in types %}
        <tr class="trash-parent editable-parent save-parent" data-type="MySchedulesTypes">
            <td class="d-none id">              {{ type.id }}   </td>
            <td class="name">                   {{ type.name }} </td>
            <td class="icon fontawesome-input"> {{ type.icon }} </td> {#todo: add icon to crud.js#}

            {% include 'modules/common/components/table-cells/actions.html.twig' with {
                "trash"       : true,
                "edit"        : true,
                "save"        : true,
                'fontawesome' : true
            } %}

        </tr>
    {% endfor %}
    </tbody>
</table>
